{
    "search page": {
        "path": "en/search.html",
        "selector": "html",
        "children": {
            "header": {
                "ref": "../components/header/header.component.json"
            },
            "main": {
                "ref": "../components/main/main.component.json"
            },
            "body results": {
                "isCollection": false,
                "selector": "#results-wrapper"
            },
            "body results headers": {
                "isCollection": true,
                "selector": "#results-wrapper h2"
            },
            "body no result message": {
                "isCollection": false,
                "selector": "div.no-result > p.ng-binding"
            }
        }
    }
}