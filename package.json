{
  "name": "page-objects-json-pattern-framework",
  "version": "1.0.0",
  "description": "Test framework based on page-objects as a json",
  "main": "index.js",
  "scripts": {
    "wd-up": "webdriver-manager update",
    "wd-st": "webdriver-manager start",
    "pretest": "del /Q .\\reports .\\logs .\\reports\\screenshots",
    "test": "./node_modules/.bin/protractor ./configs/conf.js",
    "posttest": "node ./configs/report.config.js",
    "prereport": "node ./configs/report.config.js",
    "report": ".\\reports\\Bellagio.html",
    "lint": "eslint ./ --fix",
    "preinstall": "mkdir logs .\\reports\\screenshots",
    "postinstall": "webdriver-manager update",
    "remove_dirs": "rmdir /s /q reports logs && mkdir reports\\screenshots logs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KarneyenkaDzmitry/Bellagio_resourse_json.git"
  },
  "keywords": [
    "protractor",
    "cucumber"
  ],
  "author": "Dzmitry_Karneyenka, Belarus, Minsk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KarneyenkaDzmitry/Bellagio_resourse_json/issues"
  },
  "homepage": "https://github.com/KarneyenkaDzmitry/Bellagio_resourse_json#readme",
  "dependencies": {
    "chai": "^4.2.0",
    "cucumber": "^5.0.2",
    "cucumber-json-reporter-to-html": "git+https://github.com/KarneyenkaDzmitry/cucumber-json-reporter-to-html.git",
    "protractor": "^5.4.0",
    "protractor-cucumber-framework": "^6.1.1",
    "winston": "^3.0.0",
    "yargs": "^12.0.2"
  },
  "devDependencies": {
    "eslint": "^5.7.0"
  }
}